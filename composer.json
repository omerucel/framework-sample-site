{
    "name": "omerucel/framework-sample-site",
    "require": {
        "omerucel/framework": "dev-master",
        "symfony/http-foundation": "2.3.4",
        "twig/twig": "1.14.2",
        "monolog/monolog": "1.7.0",
        "sybio/image-workshop": "2.0.5",
        "facebook/php-sdk" : "*",
        "recaptcha/php5": "dev-master",
        "swiftmailer/swiftmailer": "5.0.2"
    },
    "require-dev": {
        "phpunit/phpunit": "3.7.*"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/omerucel/framework"
        }
    ],
    "autoload": {
        "psr-0" : {
            "Application" : "src/"
        }
    }
}